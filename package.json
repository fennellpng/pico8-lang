{
  "name": "pico8-lang",
  "displayName": "pico8-lang",
  "description": "Language support for PICO-8",
  "version": "0.1.0",
  "publisher": "ianjsikes",
  "repository": "https://github.com/ianjsikes/pico8-lang",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pico-8",
        "aliases": [
          "PICO-8",
          "pico-8"
        ],
        "extensions": [
          ".lua"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pico-8",
        "scopeName": "source.pico8",
        "path": "./syntaxes/pico-8.tmLanguage.json"
      }
    ]
  }
}
